@model IEnumerable<SuperCube3D_MVC.Models.ScoreViewModel>

@{
    ViewBag.Title = "Scores";
}

<h2>Scores</h2>

@if (Model.Count() > 0)
{
    <ol>
        @foreach (var score in Model)
        {
            <li>
                @score.Result by @score.Player.UserName
            </li>
        }
    </ol>
}
else
{
    <span>No results yet...</span>
}
